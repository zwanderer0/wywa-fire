{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "dev:updates": "npm --prefix updates run dev",
    "build": "npm run build:client && npm run build:astro && npm run build:server",
    "build:client": "vite build",
    "build:astro": "npm --prefix updates run build && npm run sync:updates",
    "build:server": "vite build --config vite.config.server.ts",
    "sync:updates": "node scripts/sync-updates.mjs",
    "start": "node dist/server/node-build.mjs",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc"
  },
